#!/bin/sh
[ -z "$DEBUG" ] || set -x
set -e
YBIN="$(dirname $0)"

version=v1.3.0

bin_name=minikube \
  bin_version=${version} \
  Darwin_url=https://storage.googleapis.com/minikube/releases/${version}/minikube-darwin-amd64 \
  Darwin_sha256=5bda29e2d990bb8ac9da1767143e228772adc45507d22a49b5af70b03e7db682 \
  y-bin-dependency-download || exit $?

y-minikube-${version}-bin "$@" || echo $?
